{
    "name": "vairogs/vairogs-translatable",
    "description": "Real Latvian CMS Utils: Translatable library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "vairogs",
        "translatable",
        "symfony"
    ],
    "authors": [
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "vairogs/vairogs-core": "dev-master",
        "symfony/dependency-injection": "*",
        "symfony/http-kernel": "*",
        "symfony/config": "*",
        "symfony/http-foundation": "*",
        "sonata-project/admin-bundle": "^3",
        "sonata-project/doctrine-orm-admin-bundle": "^3",
        "doctrine/orm": "^2.6",
        "twig/twig": "^2",
        "jms/i18n-routing-bundle": "*",
        "symfony/form": "*",
        "psr/container": "^1.0",
        "doctrine/annotations": "~1.6",
        "symfony/console": "*",
        "symfony/framework-bundle": "*",
        "doctrine/dbal": "^2.8",
        "symfony/yaml": "*",
        "symfony/event-dispatcher": "*",
        "symfony/translation": "*",
        "symfony/options-resolver": "*",
        "symfony/doctrine-bridge": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\Utils\\Translatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vairogs\\Utils\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "4.1.*"
        }
    }
}
